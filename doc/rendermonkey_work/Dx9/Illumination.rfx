   rsq r3.x, r3.x
   mul r3, t3, r3.x

   ; scale and bias N
   sub r1, r1, c4.y
   mul r1, r1, c4.x

   ; N.L
   dp3_sat r2, r1, r2

   ; N.L * diffuse_light_color
   mul r2, r2, c2
   
   ; (N.H)
   dp3_sat r1, r1, r3

   ; (N.H)^k
   pow r1, r1.x, c1.x

   ; (N.H)^k * specular_light_color
   mul r1, r1, c3

   ; [(N.L) * base] + (N.H)^k
   mad_sat r0, r0, r0, r1
   
   mov oC0, r0

]]></RmShaderCode>
