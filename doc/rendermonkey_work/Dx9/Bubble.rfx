     <RmState NAME="D3DRS_ALPHABLENDENABLE" STATE="27" VALUE="1" USAGE="RENDER_STATE"/>
     <RmState NAME="D3DRS_LIGHTING" STATE="137" VALUE="0" USAGE="RENDER_STATE"/>
