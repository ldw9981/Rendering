      <RmSampler NAME="a345_map" REGISTER="-1"/>
      <RmSampler NAME="normalizer" REGISTER="-1"/>
      <RmSampler NAME="rgb_map" REGISTER="-1"/>
      <RmShaderConstant NAME="mode" REGISTER="-1"/>
     </RmHLSLShader>
