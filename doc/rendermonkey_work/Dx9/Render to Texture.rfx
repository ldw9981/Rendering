     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="ps_main"><RmShaderCode TYPE="TEXT"><![CDATA[
sampler2D RenderSample1;
sampler2D RenderSample2;

float time_0_X;

float4 ps_main( float4 inDiffuse: COLOR0, float2 inTex: TEXCOORD0 ) : COLOR0
{
   float2 TexCoord1 = float2( inTex.x + time_0_X % 20, inTex.y );
   float2 TexCoord2 = float2( inTex.x, inTex.y + time_0_X % 20 );

   return saturate ( tex2D( RenderSample1, TexCoord1 ) + 
                     tex2D( RenderSample2, TexCoord2 ) );

}



