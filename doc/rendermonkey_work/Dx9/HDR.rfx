     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="32"><RmShaderCode TYPE="TEXT"><![CDATA[
sampler2D ObjectMap;
sampler2D BumpMap;

struct PS_INPUT 
{
   float4 Pos:       POSITION;
   float2 TexCoord : TEXCOORD0;
   float3 LightDirInTangent : TEXCOORD1;
};

struct PS_OUTPUT 
{
   float4 Color : COLOR;
};

PS_OUTPUT ps_main( PS_INPUT In )
{
   PS_OUTPUT Out;
   
   float4 texColor = tex2D(ObjectMap,In.TexCoord);
   float4 bump     = tex2D(BumpMap,In.TexCoord) * 2.0 - 1.0;
   
   float lighting = max(dot(bump.xyz,-normalize(In.LightDirInTangent)),0.4);
   
   Out.Color = texColor * lighting;
   Out.Color.a = 0.0;
      
   return Out;
}



]]></RmShaderCode>
