    <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 view_proj_matrix: register(c0);
float4x4 view_matrix: register(c4);
float ShineSize: register(c8);
struct VS_OUTPUT {
   float4 Pos: POSITION;
   float3 normal: TEXCOORD0;
   float3 lightVec: TEXCOORD1;
};

VS_OUTPUT main(float4 Pos: POSITION, float3 normal: NORMAL) {
   VS_OUTPUT Out;

   // Expand the model along the normal.
   // This will allow the glow to spread better
   // without the need of extensive blurring.
   float3 p = Pos.xyz + ShineSize * normal;
   Out.Pos = mul(view_proj_matrix, float4(p, 1));

   Out.normal = mul(view_matrix, normal);
   Out.lightVec = float3(0,0,-1);

   return Out;
}





]]></RmShaderCode>
