float ring_freq;
float4 dark_wood_color;
float4 light_wood_color;
sampler pulse_train;
float4 hlsl_rings (float4 Pshade : TEXCOORD0) : COLOR
{
    float scaledDistFromZAxis = sqrt(dot(Pshade.xy, Pshade.xy)) * ring_freq;

