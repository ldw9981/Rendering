   <RmDirectXEffect NAME="Rag" API="D3D" ACTIVE_CAMERA="">
    <RmFloatVariable NAME="patternRate" API="" ARTIST_EDITABLE="TRUE" VARIABLE_SEMANTIC="" VALUE="0.060000" MIN="0.000000" MAX="1.000000" CLAMP="FALSE"/>
    <RmFloatVariable NAME="noiseRate" API="" ARTIST_EDITABLE="TRUE" VARIABLE_SEMANTIC="" VALUE="0.016000" MIN="0.000000" MAX="0.100000" CLAMP="FALSE"/>
    <RmFloatVariable NAME="noiseScale" API="" ARTIST_EDITABLE="TRUE" VARIABLE_SEMANTIC="" VALUE="0.640000" MIN="0.000000" MAX="1.000000" CLAMP="FALSE"/>
    <RmColorVariable NAME="color" API="" ARTIST_EDITABLE="TRUE" VARIABLE_SEMANTIC="" VALUE_0="0.696206" VALUE_1="1.000000" VALUE_2="0.677758" VALUE_3="1.000000"/>
    <Rm3DTextureVariable NAME="Noise" API="" ARTIST_EDITABLE="FALSE" VARIABLE_SEMANTIC="" FILE_NAME="..\Media\Textures\NoiseVolume.dds" ORIGIN="0"/>
    <RmPass NAME="Single Pass" API="D3D" PASS_INDEX="0" ENABLED="TRUE">
     <RmModelReference NAME="Terrain" API=""/>
     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_1_1" ENTRY_POINT="main" COMPILER_FLAGS="32"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 view_proj_matrix: register(c0);
float4x4 view_matrix: register(c4);
float noiseRate: register(c8);
struct VS_OUTPUT {
   float4 Pos: POSITION;
   float3 pos: TEXCOORD0;
   float3 normal: TEXCOORD1;
};

VS_OUTPUT main(float4 Pos: POSITION, float3 normal: NORMAL){
   VS_OUTPUT Out;

   // Move it down a little for a better view
   Pos.z -= 100;

   Out.Pos = mul(view_proj_matrix, Pos);
   // Pass position to the fragment shader
   Out.pos = Pos.xyz;
   // Eye-space lighting
   Out.normal = mul(view_matrix, normal);

   return Out;
}











]]></RmShaderCode>
      <RmShaderConstant NAME="view_matrix" API="" REGISTER="4"/>
      <RmShaderConstant NAME="view_proj_matrix" API="" REGISTER="0"/>
     </RmHLSLShader>
     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="main" COMPILER_FLAGS="32"><RmShaderCode TYPE="TEXT"><![CDATA[
float4 color: register(c1);
float noiseScale: register(c2);
float patternRate: register(c3);
float noiseRate: register(c4);
sampler Noise: register(s0);
float4 main(float3 pos: TEXCOORD0, float3 normal: TEXCOORD1) : COLOR {
   float2 noisy;
   noisy.x = tex3D(Noise, noiseRate * pos).r;
   noisy.y = tex3D(Noise, noiseRate * pos + 0.5).r;

   // A simple fabric style lighting
   float diffuse = 0.3 + 0.5 * dot(normal.xy, normal.xy);

   // Create a grid pattern
   float2 fp = frac(patternRate * pos.xy + noiseScale * (2 * noisy - 1));
   fp *= (1 - fp);

   return color * (fp.x + fp.y + noisy.x) * diffuse;
}





]]></RmShaderCode>
      <RmSampler NAME="Noise" API="" REGISTER="0"/>
      <RmShaderConstant NAME="color" API="" REGISTER="1"/>
      <RmShaderConstant NAME="noiseRate" API="" REGISTER="4"/>
      <RmShaderConstant NAME="noiseScale" API="" REGISTER="2"/>
      <RmShaderConstant NAME="patternRate" API="" REGISTER="3"/>
     </RmHLSLShader>
     <RmStreamMapReference NAME="PN Stream Mapping" API=""/>
     <RmTextureObject NAME="Noise" API="D3D" STATE_INDEX="0" VERTEX_TEXTURE="FALSE">
