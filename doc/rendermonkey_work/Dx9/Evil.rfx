    <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_1_1" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 view_proj_matrix: register(c0);
float4x4 view_matrix: register(c4);
float4 pos0: register(c8);
float coronaSize: register(c9);
struct VS_OUTPUT {
   float4 Pos: POSITION;
   float2 texCoord: TEXCOORD0;
};

VS_OUTPUT main(float4 Pos: POSITION){
   VS_OUTPUT Out;

   // Grab X and Y eye-space vectors in world-space ...
   float3 dirX = view_matrix[0];
   float3 dirY = view_matrix[1];

   // ... which are used for billboarding
   float3 pos = pos0 + coronaSize * (Pos.x * dirX + Pos.y * dirY);

   Out.Pos = mul(view_proj_matrix, float4(pos, 1));
   Out.texCoord = 0.5 * Pos.xy + 0.5;

   return Out;
}




]]></RmShaderCode>
