    <RmModelReference NAME="Tesselated Cube"/>
    <RmHLSLShader NAME="Vertex Shader" PIXEL_SHADER="FALSE" TARGET="vs_1_1" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 view_proj_matrix: register(c0);
float4x4 view_matrix: register(c4);
float time_0_X: register(c8);
float lerpMin: register(c9);
float lerpMax: register(c10);
float speed: register(c11);
// This shader performs a morphing between a cube and a sphere,
// like in the classical screen-saver. The source model is a
// cube with a fair amount of tesselation. The sphere is generated
// simply by normalizing the cube's vertices. Morphing between the
// keyframes are done with simple linear interpolation.

struct VS_OUTPUT {
   float4 Pos:     POSITION;
   float3 normal:  TEXCOORD0;
   float3 viewVec: TEXCOORD1;
   float4 base:    TEXCOORD2;
};

VS_OUTPUT main(float4 Pos: POSITION, float3 normal: NORMAL){
   VS_OUTPUT Out;

   // Define the two key frames
   float3 spherePos = normalize(Pos.xyz);
   float3 cubePos = 0.9 * Pos.xyz;

   float3 sphereNormal = spherePos;
   float3 cubeNormal = normal;

   // Make a smooth 0->1->0 curve
   float t = frac(speed * time_0_X);
   t = smoothstep(0, 0.5, t) - smoothstep(0.5, 1, t);

   // Find the interpolation factor
   float lrp = lerpMin + (lerpMax - lerpMin) * t;

   // Linearly interpolate the position and normal
   Pos.xyz = lerp(spherePos, cubePos, lrp);
   normal = lerp(sphereNormal, cubeNormal, lrp);

   // Use position as base color
   Out.base = 0.5 + 0.5 * Pos;
   // Give the thing some size
   Pos.xyz *= 30;

   Out.Pos = mul(view_proj_matrix, Pos);
   // Eye-space lighting
   Out.normal = mul(view_matrix, normal);
   Out.viewVec = -mul(view_matrix, Pos);

   return Out;
}

]]></RmShaderCode>
     <RmShaderConstant NAME="lerpMax" REGISTER="10"/>
     <RmShaderConstant NAME="lerpMin" REGISTER="9"/>
     <RmShaderConstant NAME="speed" REGISTER="11"/>
     <RmShaderConstant NAME="time_0_X" REGISTER="8"/>
     <RmShaderConstant NAME="view_matrix" REGISTER="4"/>
     <RmShaderConstant NAME="view_proj_matrix" REGISTER="0"/>
    </RmHLSLShader>
    <RmHLSLShader NAME="Pixel Shader" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
float4 lightDir: register(c0);
float4 main(float3 normal: TEXCOORD0, float3 viewVec: TEXCOORD1, float4 base: TEXCOORD2) : COLOR {
   normal = normalize(normal);

   // Soft diffuse
   float diffuse = 0.5 + 0.5 * dot(lightDir, normal);
   // Standard specular
   float specular = pow(saturate(dot(reflect(-normalize(viewVec), normal), lightDir)), 24);

   return diffuse * base + specular;
}


















]]></RmShaderCode>
