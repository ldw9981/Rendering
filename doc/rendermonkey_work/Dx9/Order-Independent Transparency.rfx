    <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_1_1" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 view_proj_matrix: register(c0);
struct VS_OUTPUT {
   float4 Pos: POSITION;
   float2 texCrd: TEXCOORD0;
};

VS_OUTPUT main(float4 Pos: POSITION){
   VS_OUTPUT Out;

   // Clean up inaccuracies
   Pos.xy = sign(Pos.xy);
   Out.Pos = float4(Pos.xy, 0, 1);
   // Image-space
   Out.texCrd.x = 0.5 * (1 + Pos.x);
   Out.texCrd.y = 0.5 * (1 - Pos.y);

   return Out;
}




]]></RmShaderCode></RmHLSLShader>
    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
sampler BackBack: register(s3);
sampler BackFront: register(s2);
sampler Background: register(s4);
sampler FrontBack: register(s1);
sampler FrontFront: register(s0);
float4 main(float2 texCrd: TEXCOORD0) : COLOR {
   // Sample all textures
   float4 bg = tex2D(Background, texCrd);
   float4 bb = tex2D(BackBack,   texCrd);
   float4 bf = tex2D(BackFront,  texCrd);
   float4 fb = tex2D(FrontBack,  texCrd);
   float4 ff = tex2D(FrontFront, texCrd);

   // Do transparency lerp for each layer
   float4 col = lerp(bg, bb, bb.a);
   col = lerp(col, bf, bf.a);
   col = lerp(col, fb, fb.a);
   col = lerp(col, ff, ff.a);

   return col;
}






]]></RmShaderCode>
