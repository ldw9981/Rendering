     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_1_1" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
struct VS_OUTPUT 
{
   float4 Pos     : POSITION;
   float2 texCoord: TEXCOORD0;
   float  mip     : TEXCOORD1;
};

float fSmoothness;
float viewport_width;
float viewport_height;

VS_OUTPUT main( float4 Pos: POSITION )
{
   VS_OUTPUT Out;

   // Clean up inaccuracies
   Pos.xy = sign( Pos.xy );

   Out.Pos = float4( Pos.xy, 0, 1 );
   
   float scl = viewport_width / viewport_height;
   
   // Image-space
   
   Out.texCoord.x = 0.5 * (1 + Pos.x);
   Out.texCoord.y = 0.5 * (1 - Pos.y / scl);
   
   Out.mip = ( fSmoothness * 1.) / ( viewport_width * 8.0);
   
   return Out;
}






]]></RmShaderCode>
      <RmShaderConstant NAME="fSmoothness" API="" REGISTER="0"/>
