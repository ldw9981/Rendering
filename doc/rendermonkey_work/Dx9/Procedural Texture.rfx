     </RmHLSLShader>
     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
float2 ptICircle;
float2 ptACircle;

float2 ptIStart;
float4 vIDirection;

float4 vA1Direction;
float2 ptA1Start;

float2 ptA2Start;
float4 vA2Direction;

float2 ptT1Start;
float4 vT1Direction;

float2 ptT2Start;
float4 vT2Direction;

struct PS_INPUT
{
   float2 texCoord: TEXCOORD0;
   float mip : TEXCOORD1;
};

float4 main(PS_INPUT In) : COLOR 
{
   float2 c;
      
   c = In.texCoord - (ptA1Start + vA1Direction.xy * saturate(dot(vA1Direction.zw, In.texCoord - ptA1Start)));
   float d = dot(c,c);
   c = In.texCoord - (ptA2Start + vA2Direction.xy * saturate(dot(vA2Direction.zw, In.texCoord - ptA2Start)));
   d = min(d, dot(c,c));
   c = In.texCoord - (ptT1Start + vT1Direction.xy * saturate(dot(vT1Direction.zw, In.texCoord - ptT1Start)));
   d = min(d, dot(c,c));
   c = In.texCoord - (ptT2Start + vT2Direction.xy * saturate(dot(vT2Direction.zw, In.texCoord - ptT2Start)));
   d = min(d, dot(c,c));
   c = In.texCoord - (ptIStart + vIDirection.xy * saturate(dot(vIDirection.zw, In.texCoord - ptIStart)));
   d = min(d, dot(c,c));
   d -= .001581;

   c = In.texCoord - ptACircle;
   d = min(d, dot(c,c));
   d -= .001444;

   c = In.texCoord - ptICircle;

   float2 ret = smoothstep(-In.mip, In.mip, float2(d, dot(c,c)-.001444));
   d = ret.y - ret.x;
   return float4(1,d,d,1);
}

]]></RmShaderCode>
