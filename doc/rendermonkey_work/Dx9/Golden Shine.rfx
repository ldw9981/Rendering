    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
float4 Gold: register(c1);
float ShineStrength;
float4 main(float3 normal: TEXCOORD0, float3 lightVec: TEXCOORD1) : COLOR {
   // Simple specular only lighting
   float shine = saturate(dot(normalize(lightVec), normal));
   return ShineStrength * pow(shine, 12) * Gold;
}





















]]></RmShaderCode>
     <RmShaderConstant NAME="Gold" API="" REGISTER="1"/>
     <RmShaderConstant NAME="ShineStrength" API="" REGISTER="0"/>
