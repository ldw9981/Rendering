float4x4 view_proj_matrix: register(c0);
float4 pos0: register(c4);
float eyeSize: register(c5);
struct VS_OUTPUT {
   float4 Pos: POSITION;
};

VS_OUTPUT main(float4 Pos: POSITION){
   VS_OUTPUT Out;

   // Get the eye to its size and position
   Pos.xyz = eyeSize * normalize(Pos.xyz);
   Pos.xyz += pos0;

   Out.Pos = mul(view_proj_matrix, Pos);

   return Out;
}



]]></RmShaderCode>
