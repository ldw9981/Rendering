     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="ps_main"><RmShaderCode TYPE="TEXT"><![CDATA[
// A simple workspace illustrating the usage of RenderMonkey predefined
// mouse variables (mouse buttons state and mouse coordinates). Note that the 
// coordinates are in screen coordinates (client rectangle), not in normalized device 
// coordinates. 

#include <UserData\HLSLIncludes\RmInclude.h>

float4 draw_rightM_color;
float4 draw_middleM_color;
float4 draw_leftM_color;
float4 regular_color;
float mouse_y;
float mouse_x;
float mouse_button_left;
float mouse_button_middle;
float mouse_button_right;

float4 ps_main( float4 inDiffuse: COLOR0 ) : COLOR0
{
   float4 color;
   color.xyz = 1.0f;
   color.w   = 1.0f;
   
   // Draw in "pick_color" when the left mouse button is pressed down:      
   if ( mouse_button_left == RM_MOUSE_PRESSED ) 
   {
      color = draw_leftM_color;
   }
   else if ( mouse_button_middle == RM_MOUSE_PRESSED )
   {
      color = draw_middleM_color;
   }
   else if ( mouse_button_right == RM_MOUSE_PRESSED )
   {
      color = draw_rightM_color;
   }
   else
   {
      color = regular_color;
   }
   
   return color;
}



]]></RmShaderCode></RmHLSLShader>
