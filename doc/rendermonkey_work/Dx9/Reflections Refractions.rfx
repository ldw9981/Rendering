mad  r1.rgb, c3, r1_bx2, r3  // distort base map coords by cloud coords

phase 

texld r0, r0
texld r1, r1
texld r2, t4

mov_x2 r3.rgb, r2.a           // alpha of horizon
+mul_sat r3.a, 1-r0.a, 1-r1.a // mask edges for cloud glow

