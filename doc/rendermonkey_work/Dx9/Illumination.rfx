   ; [(N.H)^2 - 0.75] * 4 == (N.H)^16
   mad r1, r1, r1, c1
   mul_sat r1, r1, c4.z

   ; (N.H)^32
   mul_sat r1, r1, r1

   ; (N.H)^32 * specular_light_color
   mul r1, r1, c3

