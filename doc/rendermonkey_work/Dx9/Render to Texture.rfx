   float4 Pos: POSITION;
   float2 Tex: TEXCOORD0;
};

VS_OUTPUT vs_main( float4 inPos: POSITION, float2 inTex: TEXCOORD0 )
{
   VS_OUTPUT Out;

   Out.Pos = mul(view_proj_matrix, inPos);
   Out.Tex = inTex;

   return Out;
