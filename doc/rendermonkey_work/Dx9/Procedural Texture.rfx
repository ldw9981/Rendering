    <Rm2DTextureVariable NAME="procedurally generated texture" API="" ARTIST_EDITABLE="FALSE" VARIABLE_SEMANTIC="" FILE_NAME="" ORIGIN="0">
     <RmPlugInData NAME="Procedural Texture Generator" API="" PLUGIN_GUID="757e2b89-9fab-4ceb-b86f-b2c2ce6d7eac"><RmTextDataBlock TYPE="TEXT"><![CDATA[
float4 TextureGradientFill( float2 vTexCoord : POSITION, 
                            float2 vTexelSize : PSIZE) : COLOR 
{
   float4 color;
   float2 sqr;
   
   sqr = 2.0f * vTexCoord - 1.0f;
   sqr *= sqr;
   
   color.a = sqrt( sqr.x + sqr.y );
   if ( color.a > 1.0f )
   {
      color.a = 1.0f - ( color.a - 1.0f );
   }
   else if ( color.a < 0.2f )
   {
      color.a = 0.2f;
   }

   color.rg = 1.0f - vTexCoord;
   color.b  = 1.0f; //vTexCoord.x * vTexCoord.y;
    
   return color;

} // end of float4 TextureGradientFill(...)]]></RmTextDataBlock>
